window/handheld/orientation=5

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-firebase/plugin.cfg", "res://addons/http-sse-client/plugin.cfg", "res://addons/json_editor/plugin.cfg")

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/stretch/scale_mode="fractional"
